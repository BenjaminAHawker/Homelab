# -------------------------
# PostgreSQL Database Config
# -------------------------
N8N_DB_HOST=n8n-db
N8N_DB_PORT=5432
N8N_DB_NAME=n8n
N8N_DB_USER=n8n
N8N_DB_PASSWORD=changeme_db_password

# -------------------------
# n8n Basic Auth
# -------------------------
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme_admin_password

# -------------------------
# Host & Webhook Settings
# -------------------------
N8N_HOST=n8n.local       # Replace with internal or external domain if needed
N8N_PORT=5678
N8N_WEBHOOK_URL=https://your-domain.example.com/  # Optional if behind reverse proxy

# -------------------------
# Timezone
# -------------------------
N8N_TIMEZONE=UTC